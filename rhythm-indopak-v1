function setup() {
  createCanvas(windowWidth, windowHeight);
  background(255);
  textAlign(CENTER, CENTER);

  let hindiFont = "नमस्ते";
  let urduFont = "نمستے";

  // hindi
  textSize(50);
  fill(0);
  text(hindiFont, width / 2, height / 2 - 40);

  // urdu
  textSize(50);
  fill(0);
  text(urduFont, width / 2, height / 2 + 40);
}
